# Production Environment Variables
NODE_ENV=production
PORT=3000

# AI Services (set your production keys)
GOOGLE_AI_API_KEY=your_production_google_ai_api_key
OPENAI_API_KEY=your_production_openai_api_key

# Security (generate strong production keys)
JWT_SECRET=your_super_secure_production_jwt_secret_32_chars_minimum
SESSION_SECRET=your_super_secure_production_session_secret
ENCRYPTION_KEY=your_32_character_production_encryption_key

# Email/Notifications
SENDGRID_API_KEY=your_production_sendgrid_api_key
SMTP_HOST=smtp.sendgrid.net
SMTP_PORT=587
SMTP_USER=apikey
SMTP_PASS=your_sendgrid_api_key

# Monitoring
NEW_RELIC_LICENSE_KEY=your_new_relic_license_key
SENTRY_DSN=your_production_sentry_dsn

# Rate limiting (production values)
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
DEBUG=

# Browser automation (production optimized)
PLAYWRIGHT_BROWSERS_PATH=/usr/bin
