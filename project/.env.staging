# Staging Environment Variables
NODE_ENV=staging
PORT=3000

# AI Services (use test/development keys)
GOOGLE_AI_API_KEY=your_staging_google_ai_api_key
OPENAI_API_KEY=your_staging_openai_api_key

# Security (different from production)
JWT_SECRET=your_staging_jwt_secret_different_from_prod
SESSION_SECRET=your_staging_session_secret
ENCRYPTION_KEY=your_staging_encryption_key_32_chars

# Email/Notifications (use staging/test accounts)
SENDGRID_API_KEY=your_staging_sendgrid_api_key
SMTP_HOST=smtp.sendgrid.net
SMTP_PORT=587
SMTP_USER=apikey
SMTP_PASS=your_staging_sendgrid_api_key

# Monitoring (staging instances)
NEW_RELIC_LICENSE_KEY=your_staging_new_relic_key
SENTRY_DSN=your_staging_sentry_dsn

# Rate limiting (more permissive for testing)
RATE_LIMIT_WINDOW_MS=300000
RATE_LIMIT_MAX_REQUESTS=200

# Logging (more verbose for testing)
LOG_LEVEL=debug
DEBUG=testgenius:*

# Browser automation
PLAYWRIGHT_BROWSERS_PATH=/usr/bin
